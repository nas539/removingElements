def fizz_buzz(max_number):
    for num in range(1, max_number + 1):
        if (num % 3 != 0) and (num % 5 != 0):
            print(num)
            continue
        if (num % 3 == 0) and (num % 5 == 0):
            print("FizzBuzz")
            continue
        if (num % 3 == 0):
            print("Fizz")
        if (num % 5 == 0):
            print("Buzz")
        

fizz_buzz(10)